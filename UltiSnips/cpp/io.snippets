
global !p
from complete import *
endglobal

snippet readfile "read file (readF)"
std::vector<char> v;
if (FILE *fp = fopen(${1:"filename"}, "r")){
	char buf[1024];
	while(size_t len = fread(buf, 1, sizeof(buf), fp))
		v.insert(v.end(), buf, buf + len);
	fclose(fp);
}
endsnippet

snippet "cout" "std::cout" rw
std::cout << ${1:output};
endsnippet

snippet "endl" "std::endl" rw
std::endl
endsnippet

