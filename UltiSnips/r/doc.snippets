endsnippet

priority 2
snippet "#'?\s*@rd(n(a(m(e)?)?)?)?" "@rdname" rb
#' @rdname ${1:target Rd doc}
endsnippet

priority 2
snippet "#'?\s*@a(u(t(h(o(r)?)?)?)?)?" "@author" rb
#' @author ${1:Ben Chuanlong Du}
endsnippet

priority 2
snippet "#'?\s*@" "Roxygen Tags" rb
#' @$1`!p snip.rv = complete(t[1], file="r/roxygen.json")`
endsnippet

priority 2
snippet "#'?\s*@t(i(t(l(e)?)?)?)?" "@title" rb
#' @title ${1:title}
endsnippet

priority 2
snippet "#'?\s*@d(e(s(c(r(i(p(t(i(o(n)?)?)?)?)?)?)?)?)?)?" "@description" rb
