snippet "m(a(t(h)?)?)?\." "Methods of math." r
math.$1`!p snip.rv = complete(t[1], "python/math.json")`
endsnippet

snippet "\bmath.abs[a-zA-Z]?" "abs()" r
abs(${1:number})
endsnippet

snippet "\bmath.max[a-zA-Z]?" "max" r
