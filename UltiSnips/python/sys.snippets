global !p
from complete import *
endglobal

snippet sys. "Methods of sys." b
sys.$1`!p snip.rv = complete(t[1], file="python/sys.json")`
endsnippet

snippet "\bsys.call_tracing[a-zA-Z]?" "sys.call_tracing" r
